    </mj-raw>
    <mj-section padding="35px" css-class="bg-white">
      <mj-divider border-color="#B3B3B3" border-width="1px" />
    </mj-section>

    <mj-raw>
      <!-- Survey -->
    </mj-raw>
    <mj-section padding="0" css-class="bg-white">
      <mj-column>
        <mj-text align="center" font-size="11.5px" color="#666666">

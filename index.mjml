        </mj-column>
        <mj-column>
          <mj-image padding="0" align="center" href="https://survey.medallia.com/?dsg-sth-comms&banner=dsg_web_dsg&email=A&transaction_id=30097238162&tracking_id=399329826058&carrier=FedEx&brand=1&sub_brand=4&score=3" src="https://i.ibb.co/JqPQRs4/three.png" alt="number three" width="50px" />
        </mj-column>
        <mj-column>
          <mj-image padding="0" align="center" href="https://survey.medallia.com/?dsg-sth-comms&banner=dsg_web_dsg&email=A&transaction_id=30097238162&tracking_id=399329826058&carrier=FedEx&brand=1&sub_brand=4&score=4" src="https://i.ibb.co/Fbf4kLT/four.png" alt="number four" width="50px" />
        </mj-column>
        <mj-column>
          <mj-image padding="0" href="https://survey.medallia.com/?dsg-sth-comms&banner=dsg_web_dsg&email=A&transaction_id=30097238162&tracking_id=399329826058&carrier=FedEx&brand=1&sub_brand=4&score=5" src="https://i.ibb.co/HnwVdt6/five.png" alt="number five" width="50px" />
        </mj-column>

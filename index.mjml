<mjml>
  <mj-head>
    <mj-style>
      * {
        box-sizing: border-box;
        font-family: Helvetica, Arial;
      }
    </mj-style>

    <mj-style inline="inline">
      .bg-white {
        background-color: #ffffff;
      }

      .rating {
        padding: 5px 115px 0;
      }

      .icons {
        padding: 0px 135px 0;
      }

      .links {
        padding: 5px 135px 0;
      }

      .link-color {
        color: #81BEFE;
      }
    </mj-style>
  </mj-head>

  <mj-body background-color="#D3D3D3">
    <mj-raw>
      <!--   Header logo -->
    </mj-raw>
    <mj-section padding="0" css-class="bg-white">
      <mj-column padding="10px" align="center" width="200px">
        <mj-image src="https://i.ibb.co/mRfQfjj/logo.png" alt="Dick's logo" />
      </mj-column>
    </mj-section>

    <mj-raw>
      <!-- Hero Image -->
    </mj-raw>
    <mj-section padding="0" css-class="bg-white">
      <mj-column>
        <mj-text align="center" font-size="16px">
          <h1>Your package is arriving soon!</h1>
        </mj-text>
      </mj-column>
    </mj-section>

    <mj-section padding="0" css-class="bg-white">
      <mj-column width="500px">
        <mj-image src="https://i.ibb.co/dWZM1JP/truck.gif" alt="truck gif" />
      </mj-column>
    </mj-section>

    <mj-raw>
      <!-- Track Package  -->
    </mj-raw>
    <mj-section padding="0" css-class="bg-white">
      <mj-column padding="5px">
        <mj-text align="left" font-size="14px" color="#808080">
          Order #: 30097238162 | Order date: Jun 7, 2023
        </mj-text>
      </mj-column>
    </mj-section>

    <mj-section padding="0" css-class="bg-white">
      <mj-column padding="5px">
